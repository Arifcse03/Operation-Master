    <![CDATA[SELECT MnjMfgOperationMasterH.HEADER_ID,
       MnjMfgOperationMasterH.SALE_ORDER_ID,
       MnjMfgOperationMasterH.SALE_ORDER_LINE_ID,
       MnjMfgOperationMasterH.INVENTORY_ITEM_ID,
